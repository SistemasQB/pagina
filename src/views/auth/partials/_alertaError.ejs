<link rel="stylesheet" href="./../styles/alerta.css">
<dialog open id="modal">
    <div id="container2">
        <div id="error-box">
            <!-- <div class="dot"></div>
            <div class="dot two"></div> -->
            <div class="face2">
                <div class="eye"></div>
                <div class="eye right"></div>
                <div class="mouth sad"></div>
            </div>
            <div class="shadow move"></div>
            <div class="message">
                <h1 class="alert">Error!</h1>
                <% for (const [key, error] of Object.entries(errores)) { %>
                    <p><%= error.msg%></p><br>
                <% } %>
                <%=// console.log(errores)%>
            </div>
            <button class="button-box" id="btnModal"><h1 class="red">Cerrar</h1></button>
        </div>
    </div>

    <!-- <footer>
        <p>made by <a href="https://codepen.io/juliepark"> julie</a> â™¡
    </footer> -->
</dialog>
<script src="./../scripts/alerta.js"></script>